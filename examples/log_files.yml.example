files: 
  - /var/log/httpd/access_log
  - /var/log/httpd/error_log
  - /opt/misc/*.log
  - /var/log/mysqld.log
  - /var/run/mysqld/mysqld-slow.log
destination:
  host: HOST.papertrailapp.com # NOTE: change this to YOUR papertrail host!
  port: 12345   # NOTE: change to your Papertrail port
  protocol: tls